{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "config": {
    "default_locale": "english",
    "locales": {
      "arabic": {
        "code": "ar",
        "code_long": "ar-AE",
        "currency": "AED"
      },
      "english": {
        "code": "en",
        "code_long": "en-US",
        "currency": "USD"
      },
      "french": {
        "code": "fr",
        "code_long": "fr-FR",
        "currency": "EUR"
      },
      "spanish": {
        "code": "es",
        "code_long": "es-ES",
        "currency": "EUR"
      }
    }
  },
  "dependencies": {
    "@vue/devtools": "5.1.1",
    "nativescript-intl": "3.0.0",
    "nativescript-socketio": "3.3.1",
    "nativescript-theme-core": "1.0.6",
    "nativescript-toasty": "1.4.0",
    "nativescript-ui-sidedrawer": "6.0.0",
    "nativescript-vue": "2.2.2",
    "nativescript-vue-devtools": "1.2.0",
    "shuutils": "3.4.0",
    "tns-core-modules": "5.4.3",
    "vue-i18n": "8.11.2",
    "vuex": "3.1.1"
  },
  "description": "A native application built with NativeScript-Vue",
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/preset-env": "7.5.0",
    "babel-eslint": "10.0.2",
    "babel-loader": "8.0.6",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "clean-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "5.0.3",
    "css-loader": "2.1.1",
    "email-validator": "2.0.4",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-html": "5.0.5",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.3",
    "lazy": "1.0.11",
    "nativescript-dev-webpack": "0.22.0",
    "nativescript-vue-template-compiler": "2.2.2",
    "nativescript-worker-loader": "0.9.5",
    "node-sass": "4.12.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "stylelint": "10.1.0",
    "stylelint-config-recommended-scss": "3.3.0",
    "stylelint-scss": "3.9.0",
    "terser-webpack-plugin": "1.3.0",
    "vue-loader": "15.7.0",
    "webpack": "4.35.2",
    "webpack-bundle-analyzer": "3.3.2",
    "webpack-cli": "3.3.5"
  },
  "engines": {
    "node": ">= 8.16.0"
  },
  "license": "GPL-3.0",
  "name": "green-app",
  "nativescript": {
    "id": "green.nsvue.app",
    "tns-android": {
      "version": "5.2.1"
    },
    "tns-ios": {
      "version": "5.0.0"
    }
  },
  "optionalDependencies": {
    "typescript": "^3.4.5"
  },
  "scripts": {
    "build": "tns build android --bundle",
    "build:ios": "export SWIFT_VERSION=3; tns build ios --bundle",
    "clean": "rimraf platforms",
    "debug": "tns debug android --bundle",
    "debug:ios": "export SWIFT_VERSION=3; tns debug ios --bundle",
    "dev": "yarn start",
    "dev:ios": "yarn start:ios",
    "last-commit": "./bin/last-commit.sh ./app/last-commit.js",
    "lint": "eslint --fix app/**/*.{js,vue} hooks/**/*.js *.js",
    "ls": "tns device",
    "postinstall": "yarn clean",
    "prebuild": "yarn last-commit",
    "prebuild:ios": "yarn last-commit",
    "prestart": "yarn last-commit",
    "prestart:ios": "yarn last-commit",
    "start": "tns run android --bundle",
    "start:ios": "export SWIFT_VERSION=3; tns run ios --bundle"
  },
  "standard": {
    "globals": [
      "TNS_ENV"
    ]
  },
  "version": "3.0.1"
}
