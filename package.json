{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/green-app/issues",
  "config": {
    "default_locale": "english",
    "locales": {
      "arabic": {
        "code": "ar",
        "code_long": "ar-AE",
        "currency": "AED"
      },
      "english": {
        "code": "en",
        "code_long": "en-US",
        "currency": "USD"
      },
      "french": {
        "code": "fr",
        "code_long": "fr-FR",
        "currency": "EUR"
      },
      "spanish": {
        "code": "es",
        "code_long": "es-ES",
        "currency": "EUR"
      }
    }
  },
  "dependencies": {
    "@teammaestro/nativescript-svg": "1.0.1",
    "@vue/devtools": "5.3.3",
    "nativescript-intl": "3.0.0",
    "nativescript-socketio": "3.3.1",
    "nativescript-toasty": "2.0.1",
    "nativescript-ui-sidedrawer": "8.0.0",
    "nativescript-vue": "2.4.0",
    "nativescript-vue-devtools": "1.2.0",
    "shuutils": "3.4.0",
    "tns-core-modules": "6.3.2",
    "vue-i18n": "8.15.3",
    "vuex": "3.1.2"
  },
  "description": "A native application built with NativeScript-Vue",
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/preset-env": "7.7.7",
    "babel-loader": "8.0.6",
    "email-validator": "2.0.4",
    "eslint": "6.8.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "11.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.1.2",
    "eslint-plugin-vue-i18n": "0.3.0",
    "lazy": "1.0.11",
    "nativescript-dev-webpack": "1.4.0",
    "nativescript-vue-template-compiler": "2.4.0",
    "nativescript-worker-loader": "0.10.0",
    "rimraf": "3.0.0",
    "vue-loader": "15.8.3"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6"
  },
  "files": [
    "app",
    "*"
  ],
  "homepage": "https://github.com/Shuunen/green-app",
  "keywords": [
    "nativescript",
    "order"
  ],
  "license": "GPL-3.0",
  "name": "green-app",
  "nativescript": {
    "id": "green.nsvue.app",
    "tns-android": {
      "version": "6.2.0"
    },
    "tns-ios": {
      "version": "6.2.0"
    }
  },
  "optionalDependencies": {
    "babel-eslint": "*",
    "typescript": "*"
  },
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/green-app.git"
  },
  "scripts": {
    "build": "tns build android --aab",
    "build:ios": "export SWIFT_VERSION=3; npm run last-commit && tns build ios",
    "check": "npx repo-check",
    "ci": "npm run test",
    "clean": "rimraf platforms",
    "debug": "tns debug android",
    "debug:ios": "export SWIFT_VERSION=3; tns debug ios",
    "dev": "npm run start",
    "dev:ios": "npm run start:ios",
    "last-commit": "bash ./bin/last-commit.sh ./app/last-commit.js",
    "lint": "eslint --fix --ignore-path .gitignore --ext .js,.vue .",
    "ls": "tns device",
    "postinstall": "npm run clean",
    "prebuild": "npm run last-commit",
    "prebuild:ios": "npm run last-commit",
    "prestart": "npm run last-commit",
    "prestart:ios": "npm run last-commit",
    "start": "tns run android --env.uglify",
    "start:ios": "export SWIFT_VERSION=3; tns run ios --env.uglify",
    "test": "npm run lint && npm run check",
    "update": "npx npm-check-updates -u"
  },
  "version": "3.3.0"
}
