{
  "name": "green-app",
  "version": "2.3.1",
  "description": "A native application built with NativeScript-Vue",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "license": "GPL-3.0",
  "engines": {
    "node": ">= 8.15.0"
  },
  "scripts": {
    "clean": "rimraf platforms",
    "dev": "tns run android --bundle",
    "dev:ios": "export SWIFT_VERSION=3; tns run ios --bundle",
    "start": "tns run android --bundle",
    "start:ios": "export SWIFT_VERSION=3; tns run ios --bundle",
    "build": "tns build android --bundle",
    "build:ios": "export SWIFT_VERSION=3; tns build ios --bundle",
    "debug": "tns debug android --bundle",
    "debug:ios": "export SWIFT_VERSION=3; tns debug ios --bundle",
    "lint": "eslint --fix app/**/*.{js,vue} hooks/**/*.js",
    "ls": "tns device",
    "postinstall": "yarn clean"
  },
  "nativescript": {
    "id": "green.nsvue.app",
    "tns-ios": {
      "version": "5.0.0"
    },
    "tns-android": {
      "version": "5.2.1"
    }
  },
  "dependencies": {
    "@vue/devtools": "5.0.0-beta.3",
    "nativescript-intl": "3.0.0",
    "nativescript-socket.io": "0.9.0",
    "nativescript-theme-core": "1.0.4",
    "nativescript-toast": "2.0.0",
    "nativescript-ui-sidedrawer": "5.1.0",
    "nativescript-vue": "2.0.2",
    "nativescript-vue-devtools": "1.1.0",
    "shuutils": "3.4.0",
    "tns-core-modules": "5.2.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "babel-loader": "8.0.5",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "clean-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "5.0.1",
    "css-loader": "1.0.1",
    "email-validator": "2.0.4",
    "eslint": "5.15.1",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-html": "5.0.3",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "lazy": "1.0.11",
    "nativescript-dev-webpack": "0.20.2",
    "nativescript-vue-template-compiler": "2.0.2",
    "nativescript-worker-loader": "0.9.5",
    "node-sass": "4.11.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "stylelint": "9.10.1",
    "stylelint-config-recommended-scss": "3.2.0",
    "stylelint-scss": "3.5.4",
    "terser-webpack-plugin": "1.2.3",
    "typescript": "3.3.3333",
    "vue-loader": "15.7.0",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "3.1.0",
    "webpack-cli": "3.2.3"
  },
  "standard": {
    "globals": [
      "TNS_ENV"
    ]
  }
}
