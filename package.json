{
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "config": {
    "default_locale": "english",
    "locales": {
      "arabic": {
        "code": "ar",
        "code_long": "ar-AE",
        "currency": "AED"
      },
      "english": {
        "code": "en",
        "code_long": "en-US",
        "currency": "USD"
      },
      "french": {
        "code": "fr",
        "code_long": "fr-FR",
        "currency": "EUR"
      },
      "spanish": {
        "code": "es",
        "code_long": "es-ES",
        "currency": "EUR"
      }
    }
  },
  "dependencies": {
    "@teammaestro/nativescript-svg": "1.0.1",
    "@vue/devtools": "5.3.3",
    "nativescript-intl": "3.0.0",
    "nativescript-socketio": "3.3.1",
    "nativescript-toasty": "1.4.0",
    "nativescript-ui-sidedrawer": "7.0.4",
    "nativescript-vue": "2.4.0",
    "nativescript-vue-devtools": "1.2.0",
    "shuutils": "3.4.0",
    "tns-core-modules": "6.2.3",
    "vue-i18n": "8.15.1",
    "vuex": "3.1.2"
  },
  "description": "A native application built with NativeScript-Vue",
  "devDependencies": {
    "@babel/core": "7.7.5",
    "@babel/preset-env": "7.7.6",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "babel-traverse": "6.26.0",
    "babel-types": "6.26.0",
    "babylon": "6.18.0",
    "clean-webpack-plugin": "1.0.1",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "2.1.1",
    "email-validator": "2.0.4",
    "eslint": "6.7.2",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-html": "6.0.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "eslint-plugin-vue": "6.0.1",
    "eslint-plugin-vue-i18n": "0.3.0",
    "lazy": "1.0.11",
    "nativescript-dev-webpack": "1.3.0",
    "nativescript-vue-template-compiler": "2.4.0",
    "nativescript-worker-loader": "0.10.0",
    "rimraf": "3.0.0",
    "terser-webpack-plugin": "2.2.3",
    "vue-loader": "15.7.2",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10"
  },
  "engines": {
    "node": ">= 10",
    "npm": ">= 6"
  },
  "license": "GPL-3.0",
  "name": "green-app",
  "nativescript": {
    "id": "green.nsvue.app",
    "templateVersion": "v2",
    "tns-android": {
      "version": "6.2.0"
    },
    "tns-ios": {
      "version": "6.2.0"
    }
  },
  "optionalDependencies": {
    "typescript": "3.7.3"
  },
  "scripts": {
    "build": "echo Please use new-version.sh to build android",
    "build:ios": "export SWIFT_VERSION=3; npm run last-commit && tns build ios",
    "check": "npx repo-check",
    "clean": "rimraf platforms",
    "debug": "tns debug android",
    "debug:ios": "export SWIFT_VERSION=3; tns debug ios",
    "dev": "npm run start",
    "dev:ios": "npm run start:ios",
    "last-commit": "bash ./bin/last-commit.sh ./app/last-commit.js",
    "lint": "eslint --fix app/**/*.{js,vue} hooks/**/*.js *.js",
    "ls": "tns device",
    "postinstall": "npm run clean",
    "prestart": "npm run last-commit",
    "prestart:ios": "npm run last-commit",
    "start": "tns run android",
    "start:ios": "export SWIFT_VERSION=3; tns run ios",
    "test": "npm run lint && npm run check",
    "update": "npx npm-check-updates -u"
  },
  "standard": {
    "globals": [
      "TNS_ENV"
    ]
  },
  "version": "3.2.0"
}
